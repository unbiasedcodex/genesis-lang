// Test visibility - public functions should work

mod math {
    fn private_add(a: i64, b: i64) -> i64 {
        a + b
    }

    pub fn public_add(a: i64, b: i64) -> i64 {
        private_add(a, b)
    }

    pub fn public_mul(a: i64, b: i64) -> i64 {
        a * b
    }
}

fn main() -> i64 {
    // This should work - public functions only
    let x: i64 = math::public_add(10, 5)
    let y: i64 = math::public_mul(x, 2)
    y
}
